generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./db.sqlite"
}

model productItem {
  id      Int    @id @default(autoincrement())
  src     String
  alt     String
  title   String
  country String
  price   String
  type    String
}

model page {
  id                   Int     @unique
  name                 String
  slug                 String  @id @unique
  type                 String
  textContentStructure String
  isDisabled           Boolean
}
